# ── Database ──────────────────────────────────────────────
DATABASE_URL=postgresql+asyncpg://webphomet:webphomet@postgres:5432/webphomet

# ── Redis / Celery ───────────────────────────────────────
REDIS_URL=redis://redis:6379/0

# ── Z.ai Agent ───────────────────────────────────────────
ZAI_API_KEY=your-z-ai-api-key
